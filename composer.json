{
    "name": "galcvua/jwt-refresh-bundle",
    "description": "Refresh JWT & logout support for Symfony security",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Galcvua\\JwtRefreshBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "galcvua"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.3",
        "symfony/framework-bundle": "^6.4 || ^7.3",
        "symfony/security-bundle": "^6.4 || ^7.3",
        "symfony/http-kernel": "^6.4 || ^7.3",
        "symfony/dependency-injection": "^6.4 || ^7.3",
        "symfony/routing": "^6.4 || ^7.3"
    },
    "require-dev": {
        "phpstan/phpstan-symfony": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.89",
        "symfony/test-pack": "^1.2"
    }
}
